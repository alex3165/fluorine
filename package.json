{
  "name": "fluorine-lib",
  "version": "1.1.1",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "description": "Reactive state and side effect management for React using a single stream of actions",
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "check": "npm run lint && npm run test",
    "clean": "rm -rf lib",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run clean && npm run build",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "test",
    "unmockedModulePathPatterns": [
      "."
    ]
  },
  "author": "Phil Pl√ºckthun <phil@plckthn.me> (https://github.com/philplckthun)",
  "bugs": {
    "url": "https://github.com/philplckthun/fluorine/issues"
  },
  "license": "MIT",
  "keywords": [
    "florine",
    "redux",
    "flux",
    "react",
    "reactivex",
    "rxjs",
    "reducer",
    "state",
    "functional",
    "immutable"
  ],
  "dependencies": {
    "babel-runtime": "^6.3.19"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-eslint": "^4.1.6",
    "babel-jest": "^6.0.1",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "eslint": "^1.10.2",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^3.11.1",
    "jest-cli": "^0.8.2",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "rxjs": "^5.0.0-beta.2"
  },
  "peerDependencies": {
    "react": ">= 0.14.0",
    "rxjs": "^5.0.0-beta.2"
  },
  "engines": {
    "node": ">= 0.12.0"
  }
}
